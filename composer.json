{
    "name": "domdanao/magpiepay-sdk-php",
    "description": "MagpiePay API for QRPh and Disbursement services",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "unlicense",
    "authors": [
        {
            "name": "OpenAPI",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MagpiePay\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MagpiePay\\Test\\": "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MagpiePay\\Laravel\\MagpiePayServiceProvider"
            ]
        }
    }
}